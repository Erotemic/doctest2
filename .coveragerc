[run]
branch = True
source = xdoctest

[report]
exclude_lines =
    pragma: no cover
    .*  # pragma: no cover
    .*  # nocover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if 0:
    if six.PY2:
    if trace is not None
    verbose = .*
    raise
    pass
    if _debug:
    if __name__ == .__main__.:
    print(.*)

omit = 
    # pytest imports this before we can cover it
    xdoctest/plugin.py

